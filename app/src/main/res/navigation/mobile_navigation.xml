<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="package com.sih.timetable.fragment.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_timetable"
        android:name="package com.sih.timetable.fragment.TimetableFragment"
        android:label="@string/title_timetable"
        tools:layout="@layout/fragment_timetable" />

    <fragment
        android:id="@+id/navigation_food"
        android:name="package com.sih.timetable.fragment.FoodFragment"
        android:label="@string/title_khana"
        tools:layout="@layout/fragment_food" />

    <fragment
        android:id="@+id/navigation_links"
        android:name="package com.sih.timetable.fragment.LinksFragment"
        android:label="@string/title_links"
        tools:layout="@layout/fragment_links" />
</navigation>